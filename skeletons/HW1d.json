{
  "descriptor": "HW1d Grader",
  "tests": {

    "compile": {
      "command": "g++ %s -o out",
      "test_must_pass": true,
      "single_file": true,
      "print_output": false,
      "point_val": 50,
      "output_match": "error",
      "negate_match": true,
      "fail_comment": "Submission did not compile"
    },
    "test_sums": {
      "command": "./out",
      "print_output": true,
      "numeric_match": [5.18738, 5.18738, 5.18738, 5.18738],
      "point_val": 10,
      "fail_comment": "Incorrect sums"
    },
    "test_float": {
      "command": "./out",
      "print_output": true,
      "numeric_match": [9.53674, -7],
      "exact_match": false,
      "point_val": 20,
      "fail_comment": "Incorrect float vals"
    },
    "test_double": {
      "command": "./out",
      "print_output": true,
      "numeric_match": [-8.88178, -16],
      "exact_match": false,
      "point_val": 20,
      "fail_comment": "Incorrect double vals"
    }
  }
}
